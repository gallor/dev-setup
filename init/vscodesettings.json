{
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 12,
    "editor.fontFamily": "Inconsolata, Menlo, Monaco, 'Courier New'",
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "relative",
    "editor.quickSuggestionsDelay": 1000,
    "editor.hover.enabled": false,
    "editor.parameterHints": false,
    "files.exclude": {
        "**/.git": true,
        "**/CVS": true,
        "**/.DS_STORE": true,
        "**/node_modules": true
    },
    "workbench.colorTheme": "Hybrid Next (Gray Background)",
    "workbench.colorCustomizations": {
        "statusBar.background": "#3c3c3c",
        "panel.background": "#2c2c2c",
        "sideBar.background": "#333333",
        "editor.background": "#2c2c2c",
        "activityBar.background": "#3c3c3c",
        "editorLineNumber.activeForeground": "#e08754",
        "editorLineNumber.foreground": "#808080"
    },
    "workbench.iconTheme": "material-icon-theme",
    "vim.leader": ",",
    "vim.hlsearch": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [";"],
            "after": [":"]
        },
        {
            "before": ["<leader>","<space>"],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": ["<leader>","l"],
            "after": ["<C-w>","l"]
        },
        {
            "before": ["<leader>","h"],
            "after": ["<C-w>","h"]
        },
        {
            "before": ["<leader>","j"],
            "after": ["<C-w>","j"]
        },
        {
            "before": ["<leader>","k"],
            "after": ["<C-w>","k"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [">"],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": ["<"],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    // "gitlens.advanced.messages": {
    //     "suppressShowKeyBindingsNotice": true
    // },
    "window.zoomLevel": 0,
    "gitlens.currentLine.enabled": false,
    // "gitlens.hovers.currentLine.over": "annotation",
    "gitlens.codeLens.enabled": false,
    // "gitlens.hovers.currentLine.enabled": false,
    "gitlens.blame.avatars": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "terminal.integrated.shellArgs.osx": []

}